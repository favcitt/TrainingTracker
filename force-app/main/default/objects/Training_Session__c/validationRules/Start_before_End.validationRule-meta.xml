<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_before_End</fullName>
    <active>true</active>
    <errorConditionFormula>Session_End_Time__c &lt;  Session_Start_Time__c</errorConditionFormula>
    <errorDisplayField>Session_End_Time__c</errorDisplayField>
    <errorMessage>No time travel allowed during trainings! Session Start Time must be before Session End Time.</errorMessage>
</ValidationRule>
